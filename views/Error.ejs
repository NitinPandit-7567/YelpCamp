<% layout('layout/boilerplate')%>
    <div class="row">
        <div class="col-8 offset-2">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading">
                    <%=message%>
                </h4>
                <p>
                    Error <%=status%>
                </p>
                <hr>
                <%if (process.env.NODE_ENV !=='production' ) {%>
                    <p class="mb-0">
                        <%=stack%>
                    </p>
                    <%}%>
            </div>
        </div>
    </div>